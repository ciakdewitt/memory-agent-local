# Anthropic API credentials
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Model configuration
LLM_MODEL=claude-3-sonnet-20240229
MAX_TOKENS=4096
TEMPERATURE=0.7

# LangGraph configuration
LANGGRAPH_CACHE_DIR=./.langgraph
LANGGRAPH_TRACING_V2=true
LANGGRAPH_PROJECT_NAME=my_agent_project

# Optional: Database connection (if needed)
# DATABASE_URL=sqlite:///langgraph.db

# Optional: Vector DB configuration (if used)
# CHROMA_PERSIST_DIRECTORY=./chroma_db
# WEAVIATE_URL=http://localhost:8080
# PINECONE_API_KEY=your_pinecone_api_key
# PINECONE_ENVIRONMENT=your_pinecone_environment

# Development environment settings
DEBUG=false
LOG_LEVEL=INFO

# Optional: Web app configuration (if exposing via API)
# HOST=127.0.0.1
# PORT=8000